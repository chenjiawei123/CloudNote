<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mycloudnote">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.mycloudnote.activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mycloudnote.activity.RegisterActivity"
            android:label="Register" />
        <activity
            android:name="com.mycloudnote.activity.NoteActivity"
            android:exported="true"
            android:label="@string/title_activity_note"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.mycloudnote.activity.NoteEditActivity"
            android:exported="true"
            android:label="Edit Note"></activity>
        <activity android:name="com.mycloudnote.activity.ChangePswActivity"></activity>
    </application>

</manifest>